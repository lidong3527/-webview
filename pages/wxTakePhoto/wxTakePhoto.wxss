camera{
  width: 100%;
  /* height: 100%; */
  position: relative;
  font-size:34rpx;
}
.takePhoto{
  position: absolute;
  left: 50%;
  bottom: 50rpx;
  transform: translate(-50%,0);
  background: rgba(255,255,255,0.5);
  border-radius: 50%;
  width: 110rpx;
  height: 110rpx;
  line-height: 100rpx;
  text-align: center;
  box-shadow: 0 0 10rpx rgba(255,255,255,0.5);
}
.photoBtn{
   width: 90rpx;
   height: 90rpx;
    border-radius: 50%;
    background:white;
    position: absolute;
    top:10rpx;
    left: 10rpx;
    font-size: 0;
    z-index: 10;
}